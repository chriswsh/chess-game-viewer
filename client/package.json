{
  "name": "chess-game-viewer",
  "author": "Chris Wong Sick Hong",
  "version": "0.0.0",
  "url": "https://github.com/chriswsh/chess-game-viewer",
  "private": true,
  "dependencies": {
    "chess.js": "^0.10.2",
    "history": "^4.6.3",
    "js-md5": "^0.6.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "0.31.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "react-test-renderer": "^15.6.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"]
  }
}
